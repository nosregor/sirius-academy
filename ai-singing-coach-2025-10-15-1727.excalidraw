    {
      "id": "dxNFv4iqSBb0q76XyUvTv",
      "type": "text",
      "x": 122.03505812348862,
      "y": 1406.8227949100242,
      "width": 1031.3014678091586,
      "height": 425,
      "angle": 0,
      "strokeColor": "#343a40",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
